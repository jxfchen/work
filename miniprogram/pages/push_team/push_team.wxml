<!--pages/push_team/push_team.wxml-->
<view>
    <view class="part_top">
        <view class="card" style="background: url(/images/task_background.png);background-size:690rpx 286rpx;">
            <view>
                <view class="team_income">团队累计收益</view>
                <image src="/images/question_mark.png" class="question_mark"></image>
                <view class="RMB_num">￥{{list.money_info.total}}</view>
            </view>
            <view class="card_main">
                <view class="transaction_orders">
                    <view class="order_text">成交推单(笔)</view>
                    <view class="order_num">{{list.total_pusher_sheet}}</view>
                </view>
                <view class="card_line1"></view>
                <view class="transaction_orders">
                    <view class="team_text">推客团队人数(人)</view>
                    <view class="team_num">{{list.total_pusher}}</view>
                </view>
                <view class="card_line2"></view>
                <view class="transaction_orders">
                    <view class="invite_text">累计邀请(人)</view>
                    <view class="invite_num">{{list.total_recommend}}</view>
                </view>
            </view>
        </view>
    </view>
    <view class="divide1"></view>
    <view class="part2">
        <image src="/images/teamlist.png" class="teamlist"></image>
        <view class="teamlist_text">团队成员列表</view>
        <view class="teamlist_details">详情</view>
        <image src="/images/arrow1.png" class="teamlist_arrow"></image>
        <block wx:key='{{list.user_list}}' wx:for='{{list.user_list}}'>
        <image class="teamlist_header1" src="{{list.user_list.avatarurl}}"></image>
        </block>
        <view class="header_num">+49</view>
    </view>
    <view class="divide1"></view>
    <view class="team_ranking">
        <image src="/images/list.png" class="list_img"></image>
        <view class="ranking_title">团队贡献排行榜</view>
        <view class="ranking_details">成员匿名说明</view>
        <image src="/images/question_mark1.png" class="question_mark1"></image>
        <view class="ranking_btn">
            <view class="manager_btn">经理推客</view>
            <view class="staff_btn">员工推客
            </view>
        </view>
        <view>
            <view>
                <view class="ranking_num">1</view>
                <image class="ranking_header" src="/images/me/header.png"></image>
                <view class="ranking_nicname">都磊素</view>
                <view class="ranking_money">累计贡献 680元
                </view>
                <view class="ranking_line"></view>
            </view>
            <view>
                <view class="ranking_num">2</view>
                <image src="/images/me/header.png" class="ranking_header"></image>
                <view class="ranking_nicname">都磊素</view>
                <view class="ranking_money">累计贡献 680元
                </view>
                <view class="ranking_line"></view>
            </view>
            <view>
                <view class="ranking_num">3</view>
                <image src="/images/me/header.png" class="ranking_header"></image>
                <view class="ranking_nicname">都磊素</view>
                <view class="ranking_money">累计贡献 680元
                </view>
                <view class="ranking_line"></view>
            </view>
            <view>
                <view class="ranking_num">4</view>
                <image src="/images/me/header.png" class="ranking_header"></image>
                <view class="ranking_nicname">都磊素</view>
                <view class="ranking_money">累计贡献 680元
                </view>
                <view class="ranking_line"></view>
            </view>
            <view>
                <view class="ranking_num">5</view>
                <image src="/images/me/header.png" class="ranking_header"></image>
                <view class="ranking_nicname">都磊素</view>
                <view class="ranking_money">累计贡献 680元
                </view>
                <view class="ranking_line"></view>
            </view>
        </view>
    </view>
</view>