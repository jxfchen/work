    margin-top: 16rpx;
}

.income {
    font-size: 24rpx;
    color: rgba(100, 100, 100, 1);
    margin: 62rpx 20rpx 0 30rpx;
    float: left;
    font-weight: bold;
}

.mon {
    font-size: 60rpx;
    font-weight: bold;
    color: rgba(243, 94, 81, 1);
    position: absolute;
    top: 526rpx;
}

.push_title {
    font-size: 40rpx;
    font-weight: bold;
    color: rgba(56, 56, 56, 1);
