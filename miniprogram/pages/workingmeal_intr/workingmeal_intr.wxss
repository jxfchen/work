}

.label3 {
    float: left;
    width: 180rpx;
    height: 40rpx;
    border-radius: 20rpx;
    border: 1rpx solid rgba(250, 96, 87, 1);
    margin-left: 10rpx;
}

.divider {
    width: 750rpx;
    height: 20rpx;
    background: rgba(246, 246, 246, 1);
}

.profit_tit {
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: rgba(46, 52, 55, 1);
    line-height: 32rpx;
}

.arrow {
    float: right;
    width: 14rpx;
    height: 28rpx;
    opacity: 0.4;
    margin: 38rpx 31rpx 0 0;
}

.profit_p1 {
    font-size: 24rpx;
    color: rgba(243, 105, 99, 1);
    line-height: 34rpx;
    float: left;
    margin-left: 26rpx;
    margin-top: 8rpx;
    margin-right: 20rpx;
}

.arrow1 {
    width: 14rpx;
    height: 22rpx;
    position: absolute;
    top: 96rpx;
    left: 684rpx;
}

.arrow2 {
    width: 14rpx;
    height: 22rpx;
    position: absolute;
    top: 195rpx;
    left: 684rpx;
}

.bottom {
    width: 750rpx;
    height: 112rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 0px 16px 0px rgba(255, 87, 76, 0.26);
    position: fixed;
    bottom: 0;
}

.btn1 {
    width: 183rpx;
    height: 50rpx;
    border: 3rpx solid #ff706b;
