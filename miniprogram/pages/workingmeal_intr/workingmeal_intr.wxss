.intr_img {
    width: 646rpx;
    height: 334rpx;
    border-radius: 6rpx;
    margin: 40rpx 52rpx 36rpx 52rpx;
}

.circular {
    width: 8rpx;
    height: 20rpx;
    background: linear-gradient(180deg, rgba(253, 140, 116, 1) 0%, rgba(246, 103, 89, 1) 100%);
    margin-top: 8rpx;
    float: left;
}

.cir_text {
    width: 648rpx;
    height: 68rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    color: rgba(100, 100, 100, 1);
    line-height: 34rpx;
    margin-left: 20rpx;
}

.label_text {
    font-size: 20rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(243, 94, 81, 1);
    line-height: 39rpx;
    text-align: center;
}

.label {
    float: left;
    width: 144rpx;
    height: 40rpx;
    border-radius: 20rpx;
    border: 1px solid rgba(250, 96, 87, 1);
}

.label2 {
    float: left;
    width: 84rpx;
